{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"target": "ES2018",
		"module": "ESNext",
		"moduleResolution": "Node",
		"lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
		"maxNodeModuleJsDepth": 0,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowJs": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"baseUrl": ".",
		"types": ["@nuxt/types", "@nuxt/typescript-build", "@types/node", "@nuxtjs/i18n"]
	},
	"exclude": ["node_modules"]
}
